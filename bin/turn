#!/usr/bin/env ruby
require_relative '../lib/turn'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]

puts "Welcome to Tic Tac Toe!"

display_board(board)

counter = 0
loop do 
  turn(board)
  counter = counter + 1
  if counter == 9
  puts "you have finished filling your board"
  break
  end
end
  